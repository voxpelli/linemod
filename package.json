  "devDependencies": {
    "@types/node": "^14.18.16",
    "@voxpelli/eslint-config": "^14.0.4",
    "@voxpelli/tsconfig": "^4.0.0",
    "dependency-check": "^5.0.0-5",
    "eslint": "^8.15.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-es": "^4.1.0",
