{
  "name": "linemod",
  "version": "2.0.0",
  "description": "Comment driven line modifications",
  "homepage": "http://github.com/voxpelli/linemod",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/linemod.git"
  },
  "type": "module",
  "bin": {
    "linemod": "cli.js"
  },
  "files": [
    "cli.js"
  ],
  "scripts": {
    "check:installed-check": "installed-check",
    "check:knip": "knip",
    "check:lint": "eslint --report-unused-disable-directives .",
    "check:tsc": "tsc",
    "check:type-coverage": "type-coverage --detail --strict --at-least 100",
    "check": "run-p check:*",
    "prepare": "husky install",
    "test:dummy-run": "node cli.js --verbose -e foo cli.js",
    "test-ci": "run-s test:*",
    "test": "run-s check test:*"
  },
  "keywords": [],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "MIT",
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "linemod-core": "^2.0.2",
    "meow": "^12.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.19.40",
    "@voxpelli/eslint-config": "^20.0.0",
    "@voxpelli/tsconfig": "^12.0.1",
    "eslint": "^9.7.0",
    "husky": "^9.1.1",
    "installed-check": "^9.3.0",
    "knip": "^5.26.0",
    "npm-run-all2": "^6.2.2",
    "type-coverage": "^2.29.1",
    "typescript": "~5.5.3",
    "validate-conventional-commit": "^1.0.4"
  }
}
