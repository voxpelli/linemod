{
  "name": "linemod",
  "version": "0.2.0",
  "description": "Comment driven line modifications",
  "homepage": "http://github.com/voxpelli/linemod",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/linemod.git"
  },
  "type": "module",
  "bin": {
    "linemod": "cli.js"
  },
  "files": [
    "cli.js"
  ],
  "scripts": {
    "check:dependency-check": "dependency-check *.js --no-dev",
    "check:installed-check": "installed-check -i eslint-plugin-jsdoc",
    "check:eslint": "eslint --report-unused-disable-directives .",
    "check:tsc": "tsc",
    "check": "run-p check:*",
    "prepare": "husky install",
    "test:dummy-run": "node cli.js --verbose -e foo cli.js",
    "test-ci": "run-s test:*",
    "test": "run-s check test:*"
  },
  "keywords": [],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "MIT",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "linemod-core": "^0.2.0",
    "meow": "^10.1.2"
  },
  "devDependencies": {
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@types/node": "^14.18.21",
    "@voxpelli/eslint-config": "^14.1.0",
    "@voxpelli/tsconfig": "^4.0.0",
    "dependency-check": "^5.0.0-7",
    "eslint": "^8.18.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-unicorn": "^43.0.1",
    "husky": "^8.0.1",
    "installed-check": "^6.0.1",
    "npm-run-all2": "^6.0.1",
    "typescript": "~4.6.4"
  }
}
